ack(async (id: string) => {\n        const res = window.confirm(\"この画像を削除しますか？\")\n        if (!res) {\n            return false\n        } else {\n            const newImages = images.filter((image: Image) => image.id !== id)\n            setImages(newImages)\n            return storage.ref('images').child(id).delete()\n        }\n    }, [images])\n\n    return (\n        <>\n            <div className=\"text-right\">\n                <span>画像を登録</span>\n                <IconButton className={classes.icon}>\n                    <label>\n                        <AddPhotoAlternate />\n                        <input id=\"image\" className=\"display-none\"\n                               type=\"file\" onChange={(e) => uploadImage(e)}\n                        />\n                    </label>\n                </IconButton>\n            </div>\n            <div className=\"imageList\">\n                {images.length > 0 && (\n                    images.map((image: Image) =>\n                        <ImagePreview\n                            key={image.id} id={image.id}\n                            path={image.path} deleteImage={deleteImage}\n                        />\n                    )\n                )}\n            </div>\n        </>\n    )\n}\n\nexport default ImageArea\n","/Users/yukiishihara/Desktop/TypeScript-App/food-report/src/pages/SignUp.tsx",[],["92","93"],"/Users/yukiishihara/Desktop/TypeScript-App/food-report/src/reducks/users/operations.ts",[],"/Users/yukiishihara/Desktop/TypeScript-App/food-report/src/pages/ReportList.tsx",["94"],"/Users/yukiishihara/Desktop/TypeScript-App/food-report/src/components/reports/ReportCard.tsx",[],"/Users/yukiishihara/Desktop/TypeScript-App/food-report/src/reducks/reports/operations.ts",[],"/Users/yukiishihara/Desktop/TypeScript-App/food-report/src/pages/ReportEdit.tsx",["95"],"/Users/yukiishihara/Desktop/TypeScript-App/food-report/src/components/reports/ImageSwiper.tsx",[],"/Users/yukiishihara/Desktop/TypeScript-App/foo