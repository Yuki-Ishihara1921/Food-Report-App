'\nimport { makeStyles, IconButton } from '@material-ui/core'\nimport { AddPhotoAlternate } from '@material-ui/icons'\n\ntype Props = {\n    images: Image[]\n    setImages: React.Dispatch<React.SetStateAction<Image[]>>\n}\n\nconst useStyles = makeStyles({\n    icon: {\n        marginRight: 8,\n        height: 48,\n        width: 48\n    }\n})\n\nconst ImageArea: FC<Props> = ({images, setImages}) => {\n    const classes = useStyles()\n\n    const uploadImage = useCallback((e) => {\n        const file = e.target.files\n        let blob = new Blob(file, { type: \"image/jpeg\" })\n        const S = \"abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789\"\n        const N = 16\n        const fileName = Array.from(crypto.getRandomValues(new Uint32Array(N))).map((n) => S[n % S.length]).join('')\n        const uploadRef = storage.ref('images').child(fileName)\n        const uploadTask = uploadRef.put(blob)\n        uploadTask.then(() => {\n            uploadTask.snapshot.ref.getDownloadURL().then((downloadURL) => {\n                const newImage: Image = { id: fileName, path: downloadURL }\n                setImages(((prevState) => [...prevState, newImage]))\n            })\n        })\n    }, [setImages])\n\n    const deleteImage = useCallback(async (id: string) => {\n        const res = window.confirm(\"この画像を削除しますか？\")\n        if (!res) {\n            return false\n        } else {\n            const newImages = images.filter((image: Image) => image.id !== id)\n            setImages(newImages)\n            return storage.ref('images').child(id).delete()\n        }\n    }, [images])\n\n    return (\n        <>\n            <div className=\"text-right\">\n                <span>画像を登録</span>\n                <IconButton className={classes.icon}>\n                    <label>\n                        <AddPhotoAlternate />\n                        <input id=\"image\" className=\"display-none\"\n                               type=\"file\" onChange={(e) => uploadImage(e)}\n                        />\n                    </label>\n                </IconButton>\n            </div>\n            <div className=\"list-images\">\n                {images.length > 0 && (\n                    images.map((image: Image) =>\n                        <ImagePreview\n                            key={image.id} id={image.id}\n                            path={image.path} deleteImage={deleteImage}\n                        />\n                    )\n                )}\n            </div>\n        </>\n    )\n}\n\nexport default ImageArea\n","/Users/yukiishihara/Desktop/TypeScript/food-report/src/templates/ReportEdit.tsx",["115"],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/App.tsx",[],["116","117"],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/templates/ReportDetail.tsx",["118"],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/reducks/reports/operations.ts",[],["119","120"],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/components/header/SideBarDrawer.tsx",["121"],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/components/header/Header.tsx",[],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/templates/ReportList.tsx",["122"],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/components/UI/SelectBox.tsx",[],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/components/header/AppLogo.tsx",["123"],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/Router.tsx",[],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/reducks/reports/actions.ts",[],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/components/UI/TextInput.tsx",[],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/templates/SignUp.tsx",[],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/components/reports/ImageSwiper.tsx",[],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/components/reports/ImagePreview.tsx",[],"/Users/yukiishihara/Desktop/TypeScript/food-report/src/templates/SignIn.tsx",[],"/Use